{
  "name": "elina",
  "version": "0.5.2",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "build-angular": "rm -rf dist && ng build --base-href .",
    "copy-electron": "cp -a src-electron/* dist/raw && cp -a src-distro/* dist/raw",
    "build-electron": "npm run build-angular && npm run copy-electron",
    "electron": "npm run build-electron && electron dist/raw",
    "pack-and-setup": "npm run build-electron && cd dist/raw && npm run exe32 && npm run set32"
  },
  "dependencies": {
    "@angular/common": "^4.3.6",
    "@angular/compiler": "^4.3.6",
    "@angular/core": "^4.3.6",
    "@angular/forms": "^4.3.6",
    "@angular/http": "^4.3.6",
    "@angular/platform-browser": "^4.3.6",
    "@angular/platform-browser-dynamic": "^4.3.6",
    "@angular/router": "^4.3.6",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.1",
    "file-saver": "^1.3.3",
    "ngx-bootstrap": "^1.9.1",
    "ngx-electron": "^1.0.3",
    "pouchdb": "~6.2.0",
    "pouchdb-find": "~6.2.0",
    "pouchdb-upsert": "^2.2.0",
    "rxjs": "^5.4.3",
    "xlsx": "^0.11.3",
    "zone.js": "^0.8.17"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.2",
    "@angular/compiler-cli": "^4.3.6",
    "@types/file-saver": "0.0.1",
    "@types/node": "^8.0.26",
    "@types/pouchdb": "~6.1.1",
    "@types/pouchdb-find": "^6.3.0",
    "@types/pouchdb-upsert": "^2.0.3",
    "codelyzer": "^3.1.2",
    "electron": "^1.7.5",
    "electron-context-menu": "^0.9.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.7.0",
    "typescript": "^2.5.2"
  }
}
