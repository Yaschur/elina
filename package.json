{
  "name": "elina",
  "version": "0.7.2",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "build-angular": "rm -rf dist && ng build --base-href .",
    "copy-electron": "cp -a src-electron/* dist/raw && cp -a src-distro/* dist/raw",
    "build-electron": "npm run build-angular && npm run copy-electron",
    "electron": "npm run build-electron && electron dist/raw",
    "pack-and-setup": "npm run build-electron && cd dist/raw && npm run exe32 && npm run set32"
  },
  "dependencies": {
    "@angular/common": "^5.0.0",
    "@angular/compiler": "^5.0.0",
    "@angular/core": "^5.0.0",
    "@angular/forms": "^5.0.0",
    "@angular/http": "^5.0.0",
    "@angular/platform-browser": "^5.0.0",
    "@angular/platform-browser-dynamic": "^5.0.0",
    "@angular/router": "^5.0.0",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.1",
    "file-saver": "^1.3.3",
    "ngx-bootstrap": "^1.9.3",
    "ngx-electron": "^1.0.3",
    "pouchdb": "~6.2.0",
    "pouchdb-find": "~6.2.0",
    "pouchdb-upsert": "^2.2.0",
    "rxjs": "^5.5.2",
    "xlsx-populate": "^1.11.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.5.0",
    "@angular/compiler-cli": "^5.0.0",
    "@types/node": "^8.0.47",
    "@types/file-saver": "0.0.1",
    "@types/pouchdb": "~6.1.1",
    "@types/pouchdb-find": "^6.3.0",
    "@types/pouchdb-upsert": "^2.0.3",
    "codelyzer": "^4.0.0",
    "electron": "^1.7.9",
    "electron-context-menu": "^0.9.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1"
  }
}
