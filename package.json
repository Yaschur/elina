{
  "name": "elina",
  "version": "0.5.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "build-angular-copy-electron": "rm -rf dist && ng build --base-href . && cp -a src-electron/* dist",
    "build-electron": "npm run build-angular-copy-electron && cd dist && npm install",
    "electron": "npm run build-angular-copy-electron && electron dist",
    "bbb1": "rm -rf dist && ng build --base-href . -op dist/temp/clt && cp -a src-electron/* dist/temp",
    "bbb2": "npm run bbb1 && cd dist/temp && npm install",
    "bbb": "npm run bbb2 && electron-packager dist/temp elina --platform win32 --arch ia32 --out dist/",
    "iii": "electron-installer-windows --src dist/elina-win32-ia32/ --dest dist/installers/"
  },
  "dependencies": {
    "@angular/common": "^4.3.4",
    "@angular/compiler": "^4.3.4",
    "@angular/core": "^4.3.4",
    "@angular/forms": "^4.3.4",
    "@angular/http": "^4.3.4",
    "@angular/platform-browser": "^4.3.4",
    "@angular/platform-browser-dynamic": "^4.3.4",
    "@angular/router": "^4.3.4",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.0",
    "ngx-bootstrap": "^1.8.1",
    "ngx-electron": "^1.0.3",
    "pouchdb": "~6.2.0",
    "pouchdb-find": "~6.2.0",
    "pouchdb-upsert": "^2.2.0",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.0",
    "@angular/compiler-cli": "^4.3.4",
    "@types/node": "^8.0.20",
    "@types/pouchdb": "~6.1.1",
    "@types/pouchdb-find": "^6.3.0",
    "@types/pouchdb-upsert": "^2.0.3",
    "codelyzer": "^3.1.2",
    "electron": "^1.7.5",
    "electron-context-menu": "^0.9.1",
    "electron-installer-windows": "^0.2.0",
    "electron-packager": "^8.7.2",
    "ts-node": "^3.3.0",
    "tslint": "^5.6.0",
    "typescript": "^2.4.2"
  }
}
