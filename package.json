{
	"name": "elina",
	"version": "0.5.1",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"lint": "ng lint",
		"build-angular-copy-electron": "rm -rf dist && ng build --base-href . && cp -a src-electron/* dist",
