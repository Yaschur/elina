		"@angular/router": "^4.3.4",
		"bootstrap": "^3.3.7",
		"core-js": "^2.5.0",
		"ngx-bootstrap": "^1.8.1",
		"ngx-electron": "^1.0.3",
		"pouchdb": "~6.2.0",
		"pouchdb-find": "~6.2.0",
