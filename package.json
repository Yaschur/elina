    "@types/pouchdb": "^6.3.2",
    "@types/pouchdb-find": "^6.3.2",
    "@types/pouchdb-upsert": "^2.2.3",
    "codelyzer": "^4.1.0",
    "electron": "^1.8.2",
    "electron-context-menu": "^0.9.1",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "typescript": "^2.7.1",
    "webpack": "^3.11.0"
  }
