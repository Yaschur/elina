{
  "name": "elina",
  "version": "0.8.4",
  "scripts": {
    "ng": "ng",
    "lint": "ng lint",
    "build-angular": "rm -rf dist && ng build --base-href .",
    "copy-electron": "cp -a src-electron/* dist/raw && cp -a src-distro/* dist/raw",
    "build-electron": "npm run build-angular && npm run copy-electron",
    "electron": "npm run build-electron && electron dist/raw",
    "pack-and-setup": "npm run build-electron && cd dist/raw && npm run exe64 && npm run set64"
  },
  "dependencies": {
    "@angular/common": "^5.2.4",
    "@angular/compiler": "^5.2.4",
    "@angular/core": "^5.2.4",
    "@angular/forms": "^5.2.4",
    "@angular/http": "^5.2.4",
    "@angular/platform-browser": "^5.2.4",
    "@angular/platform-browser-dynamic": "^5.2.4",
    "@angular/router": "^5.2.4",
    "angular-2-dropdown-multiselect": "^1.6.3",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.1",
    "file-saver": "^1.3.3",
    "ngx-bootstrap": "^1.9.3",
    "ngx-electron": "^1.0.4",
    "pouchdb": "^6.4.3",
    "pouchdb-find": "^6.4.3",
    "pouchdb-upsert": "^2.2.0",
    "rxjs": "^5.5.6",
    "xlsx-populate": "^1.14.0",
    "zone.js": "^0.8.18"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.8",
